{
  "appId": "com.oryza.plantai",
  "appName": "ORYZA",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "Camera": {
      "permissions": ["camera", "photos", "storage"]
    },
    "StatusBar": {
      "style": "light",
      "backgroundColor": "#10b981"
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#f8fafc",
      "showSpinner": true,
      "spinnerColor": "#10b981"
    }
  },
  "android": {
    "allowMixedContent": true,
    "buildOptions": {
      "keystorePath": "",
      "keystoreAlias": ""
    }
  }
}